<% @jokes.each do |joke| %>
<div>
	<h1> <%= joke.user.email %></h1>
	<h2> <%= joke.body %></h2>
	<p>  <%= joke.score %></p>
	<%= link_to "upvote", like_joke_path(joke), method: "put" %>
	
	<%= link_to "downvote", dislike_joke_path(joke), method: "put" %>
	<%#=form_tag("/jokes/#{joke.id}/votes/downvote", method: "post") do %>
	<%#= submit_tag("downvote") %>
	
</div>
	<% end %>